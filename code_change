import pandas as pd

# Load the CSV file into a pandas DataFrame
file_path = 'your_file.csv'  # Replace with the actual file path
df = pd.read_csv(file_path)

# Create a new DataFrame to store the synthetic data
synthetic_df = pd.DataFrame(columns=['instruction', 'output'])

# Iterate through each row in the original DataFrame
for index, row in df.iterrows():
    # Copy the 'output' value
    output_value = row['output']

    # Generate four synthetic rows with empty 'instruction' column
    for _ in range(4):
        synthetic_df = synthetic_df.append({'instruction': '', 'output': output_value}, ignore_index=True)

# Save the synthetic data to a new CSV file
synthetic_df.to_csv('synthetic_data.csv', index=False)

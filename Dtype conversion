import pandas as pd

# Sample DataFrame
data = {
    'col1': [1, 2, 'a', 'b', True],
    'col2': ['x', 'y', 3, 4, False]
}
df = pd.DataFrame(data)

# Function to convert column to int if possible
def convert_to_int(column):
    try:
        return column.astype(int)
    except ValueError:
        return column

# Function to convert column to string if possible
def convert_to_string(column):
    try:
        return column.astype(str)
    except ValueError:
        return column

# Iterate over each column
for col in df.columns:
    # Check if any entry is numeric or string
    if any(pd.to_numeric(df[col], errors='coerce').notnull()) or any(df[col].apply(lambda x: isinstance(x, str))):
        # Convert to int if possible
        df[col] = convert_to_int(df[col])
        # Convert to string if not int
        df[col] = convert_to_string(df[col])

# Display the modified DataFrame
print(df)
